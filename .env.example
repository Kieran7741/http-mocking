# Copy to .env.local and adjust for your cluster/dev services.

# Port to run the local proxy on
PROXY_PORT=5175

# Remote API endpoints (dev/stage routes, or localhost if you port-forward)
USERS_SERVICE_URL=https://dev.example.com/users
ORDERS_SERVICE_URL=https://dev.example.com/orders

# Flip individual services to use local MSW mocks instead of proxying
MOCK_USERS=false
MOCK_ORDERS=false
